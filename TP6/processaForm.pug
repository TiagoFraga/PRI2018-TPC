extends layout
block content
    .w3-container
        .w3-card-4
            header.w3-container.w3-teal
                h3 Tese Recebida com Sucesso !!!
            .w3-container
                table.w3-table-all
                    tr
                        th Aluno
                        th Informação da Tese
                    tr
                        td
                            table.w3-table-all
                                tr
                                    th Nome:
                                    td= tese.nome
                                tr
                                    th Número / Identificador:
                                    td= tese.numero
                                tr
                                    th Curso:
                                    td= tese.curso
                                tr
                                    th Instituição:
                                    td= tese.inst
                        td
                            table.w3-table-all
                                tr
                                    th Titulo:
                                    td= tese.titulo
                                tr
                                    th Data:
                                    td= tese.data
                                tr
                                    th Tipo:
                                    td= tese.tipo
            footer.w3-container.w3-teal
                p
                    a(href='http://localhost:4006/')
                        | Voltar à Página Principal
                address Gerado por Gerado por Registo de Teses App de TMF.